<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tunasteak.demo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tunasashimi"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name="com.tunasteak.activity.EntryBindingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.tunasteak.activity.DataShowBindingActivity" />
        <activity android:name="com.tunasteak.activity.TwoWayBindingActivity" />

        <activity android:name="com.tunasteak.activity.ChooseSingleBindingActivity" />
        <activity android:name="com.tunasteak.activity.ChooseMultipleBindingActivity" />
        <activity android:name="com.tunasteak.activity.ExpandableListViewBindingActivity" />
        <activity android:name="com.tunasteak.activity.GridViewBindingActivity" />
        <activity android:name="com.tunasteak.activity.RecyclerViewBindingActivity" />

        <activity android:name="com.tunasteak.activity.TLoadBindingActivity" />
    </application>
</manifest>